msgid "From the command line"
msgstr "Depuis la ligne de commande"

#. type: Plain text
#: docs/admin/55.upgrade/11.0-bullseye.mdx:26
#, no-wrap
msgid "After upgrading to 4.4.x, run:\n"
msgstr ""

#. type: Plain text
#: docs/admin/55.upgrade/11.0-bullseye.mdx:32
#: docs/admin/55.upgrade/12.0-bookworm/index.mdx:38
#, no-wrap
