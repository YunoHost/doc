msgstr "<TabItem value=\"usbimager\" label=\"con USBimager\">\n"

#. type: Plain text
#: docs/admin/05.get_started/20.install_on/arm_board.mdx:60
#: docs/admin/05.get_started/20.install_on/raspberry_pi.mdx:59
#: docs/admin/05.get_started/20.install_on/regular_computer.mdx:57
#, no-wrap
msgid "Download [USBimager](https://bztsrc.gitlab.io/usbimager/) for your operating system and install it.\n"
msgstr ""
"Descarga [USBimager](https://bztsrc.gitlab.io/usbimager/) para o teu sistema "
"operativo e instálaa.\n"

#. type: Plain text
#: docs/admin/05.get_started/20.install_on/arm_board.mdx:62
#: docs/admin/05.get_started/20.install_on/raspberry_pi.mdx:61
#, no-wrap
msgid "Plug your SD card, select your image and click \"Write\"\n"
msgstr "Conecta a tarxeta SD, selecciona a imaxe e preme en \"Escribir\"\n"

#. type: Plain text
#: docs/admin/05.get_started/20.install_on/arm_board.mdx:64
#: docs/admin/05.get_started/20.install_on/raspberry_pi.mdx:63
