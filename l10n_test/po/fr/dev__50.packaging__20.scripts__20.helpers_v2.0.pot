"Example of line this helpers can managed correctly\n"
".yml\n"
"    title: YunoHost documentation\n"
"    email: 'yunohost@yunohost.org'\n"
".json\n"
"    \"theme\": \"colib'ris\",\n"
"    \"port\": 8102\n"
"    \"some_boolean\":     false,\n"
"    \"user\": null\n"
".ini\n"
"    some_boolean = On\n"
"    action = \"Clear\"\n"
"    port = 20\n"
".php\n"
"    $user=\n"
"    user => 20\n"
".py\n"
"    USER = 8102\n"
"    user = 'https://donate.local'\n"
